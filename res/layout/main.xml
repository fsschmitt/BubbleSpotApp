<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
   <com.bubblespot.Header android:id="@+id/header"
		android:layout_width="match_parent" android:layout_height="wrap_content" />
    <LinearLayout android:layout_width="match_parent" android:id="@+id/linearLayout4" android:layout_height="wrap_content" android:orientation="vertical">
        <ImageView android:id="@+id/cima" android:src="@drawable/cima" android:layout_width="wrap_content" android:layout_weight="1" android:layout_height="0dp" android:padding="2dp"></ImageView>
        <LinearLayout android:layout_width="match_parent" android:id="@+id/linearLayout2" android:layout_weight="1" android:layout_height="0dp">
            <ImageView android:id="@+id/centro_esq" android:src="@drawable/meio_esq" android:layout_height="wrap_content" android:layout_weight="1" android:layout_width="0dp" android:padding="2dp"></ImageView>
            <LinearLayout android:orientation="vertical" android:id="@+id/linearLayout3" android:layout_height="match_parent" android:layout_weight="1" android:layout_width="0dp">
                <ImageView android:id="@+id/centro_dir_cima" android:src="@drawable/meio_dir_cima" android:layout_width="wrap_content" android:layout_weight="1" android:layout_height="0dp" android:padding="2dp"></ImageView>
                <ImageView android:id="@+id/centro_dir_baixo" android:src="@drawable/meio_dir_baixo" android:layout_width="wrap_content" android:layout_weight="1" android:layout_height="0dp" android:padding="2dp"></ImageView>
            </LinearLayout>
        </LinearLayout>
        <ImageView android:id="@+id/baixo" android:src="@drawable/baixo" android:layout_width="wrap_content" android:layout_weight="1" android:layout_height="0dp" android:padding="2dp"></ImageView>
    </LinearLayout>
    
</LinearLayout>
